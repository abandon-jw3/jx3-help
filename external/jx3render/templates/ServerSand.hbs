      }

      .u-name {
        display: block;
        color: #fffcd0;
        font-size: 12px;
        line-height: 18px;
        width: 1em;
        text-shadow: 1px 1px 1px #000;
      }

      .server {
        position: absolute;
        max-width: 100%;
        left: 50%;
        transform: translate(-50%, 0);
        top: 35px;
        z-index: 1;
      }

      .updateTime {
        position: absolute;
        max-width: 100%;
        left: 50%;
        transform: translate(-50%, 0);
        top: 65px;
        z-index: 1;
      }

      .server .text {
        font-weight: 800;
        font-size: 36px;
        position: relative;
        color: #f4d8b3;
      }

      .updateTime .text {
        font-weight: 600;
        font-size: 25px;
        position: relative;
        color: #f4d8b3;
      }

      .text::after {
        content: attr(text);
        position: absolute;
        top: 0;
        left: 0;
        z-index: 10;
        background-image: linear-gradient(to bottom, #ffeebc, #ce893b);
        -webkit-background-clip: text;
        background-clip: text;
        color: transparent;
        text-shadow: none;
      }
    </style>
  </head>

